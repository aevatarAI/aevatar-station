{
  "Serilog": {
    "Properties": {
      "Application": "Aevatar.Silo"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Orleans": "Information",
      "Orleans.Runtime": "Information",
      "Orleans.Providers.MongoDB": "Information",
      "Orleans.Streams": "Warning",
      "Orleans.Kafka": "Warning"
    }
  },
  "Orleans": {
    "ClusterId": "AevatarSiloCluster",
    "ServiceId": "AevatarBasicService",
    "MongoDBClient": "mongodb://localhost:27017",
    "DataBase": "AevatarDb",
    "AdvertisedIP": "127.0.0.1",
    "SiloPort": 11111,
    "GatewayPort": 30000,
    "DashboardPort": 8080,
    "DashboardUserName": "admin",
    "DashboardPassword": "admin",
    "DashboardCounterUpdateIntervalMs": 1000
  },
  "ConnectionStrings": {
    "Default": "mongodb://127.0.0.1:27017/Aevatar"
  },
  "OrleansStream": {
    "Provider": "Memory"
  },
  "OrleansEventSourcing": {
    "Provider": "Mongodb"
  },
  "AgentWarmup": {
    "Enabled": false
  },
  "Host": {
    "HostId": "DevHost",
    "Version": "1.0.0"
  }
} 